#include <stdio.h>

int main()
{
    int size,a;
    scanf("%d",&size);
    int arr[size];
    for(int i=0;i<size;i++)
    scanf("%d",&arr[i]);
    int key;
    scanf("%d",&key);
    for(int i=0;i<size;i++)
    {
     if(arr[i]==key)
     {
     printf("%d",i);
     a=1;
     }
     }
    if(a!=1){
        printf("%d",-1);
    }
 return 0;
}
