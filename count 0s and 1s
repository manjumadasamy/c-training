#include <stdio.h>

int main()
{
    int size,count=0,count1;
    scanf("%d",&size);
    int arr[size];
    for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
    for(int index=0;index<size;index++)
    {
        if(arr[index]==0)
        count++;
        else
        count1++;
    }
        printf("%d %d",count,count1);
    return 0;
}
