#include <stdio.h>

int main()
{
    char str[]="rvsctsr";
    char alphabets[123]={0};
     int i=0;
    for(i=0;str[i]!='\0';i++)
    {
        alphabets[str[i]]++;
    }
    for(i=0;i<123;i++)
     {
         if(alphabets[i]>=1)
         {
       printf("\n%c %d",i,alphabets[i]);
       break;
         }}
    return 0;
}
