#include <stdio.h>

int main()
{
    char str[50];
    fgets(str,sizeof(str),stdin);
    int i=0,vowels=0,constant=0;
    for(i=0;str[i]!='\0';i++)
    {
        if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='u')
        vowels++;
        else
        constant++;
        }
        printf("%d %d",vowels,constant);
    return 0;
}
